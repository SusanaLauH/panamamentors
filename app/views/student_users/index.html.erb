<h1>Listing student_users</h1>

<table>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Email</th>
    <th>Password</th>
    <th>Studies level</th>
    <th>Field</th>
    <th>Interests</th>
    <th>Mentor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @student_users.each do |student_user| %>
  <tr>
    <td><%= student_user.firstname %></td>
    <td><%= student_user.lastname %></td>
    <td><%= student_user.email %></td>
    <td><%= student_user.password %></td>
    <td><%= student_user.studies_level %></td>
    <td><%= student_user.field %></td>
    <td><%= student_user.interests %></td>
    <td><%= student_user.mentor %></td>
    <td><%= link_to 'Show', student_user %></td>
    <td><%= link_to 'Edit', edit_student_user_path(student_user) %></td>
    <td><%= link_to 'Destroy', student_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

